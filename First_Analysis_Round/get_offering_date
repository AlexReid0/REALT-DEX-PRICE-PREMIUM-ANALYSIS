import time
import concurrent.futures



def scrape_property_data(base_url):
    try:
        # Scrape the Offering Date
        offering_url = base_url + "#tab-title-offering_tab"
        response_offering = requests.get(offering_url)
        soup_offering = BeautifulSoup(response_offering.content, 'html.parser')
        offering_date_element = soup_offering.find("li", class_="assetOfferingDate")
        offering_date = offering_date_element.find("span", class_="data").text.strip() if offering_date_element else "Not found"

        # Scrape the Token Identifier from the main product page
        response_product = requests.get(base_url)
        soup_product = BeautifulSoup(response_product.content, 'html.parser')
        ethereum_button_element = soup_product.find("a", class_="ethereum-button xdai")
        token_link = ethereum_button_element['href'] if ethereum_button_element else "Not found"
        token_identifier = token_link.split('/')[-1] if ethereum_button_element else "Not found"

        return token_identifier, offering_date
    except Exception as e:
        print(f"An error occurred while processing URL {base_url}: {e}")
        return "Error", "Error"

