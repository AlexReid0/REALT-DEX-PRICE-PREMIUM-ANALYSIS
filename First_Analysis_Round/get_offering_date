print(property_base_urls)


# Use ThreadPoolExecutor to scrape data concurrently
data = []
with concurrent.futures.ThreadPoolExecutor(max_workers=10) as executor:
    future_to_url = {executor.submit(scrape_property_data, url): url for url in property_base_urls}
    for future in concurrent.futures.as_completed(future_to_url):
        url = future_to_url[future]  # Get the URL associated with this future
        try:
            identifier, offering_date = future.result()  # Get the results from the future
            data.append([url, identifier, offering_date])
            print(f"Scraped data from {url} - Identifier: {identifier}, Offering Date: {offering_date}")
        except Exception as exc:
            print(f"An exception occurred for URL {url}: {exc}")

# Write data to CSV
print("Writing data to CSV file...")
with open('properties_data.csv', 'w', newline='', encoding='utf-8') as file:
    writer = csv.writer(file)
    writer.writerow(['URL', 'Identifier', 'Offering Date'])
    writer.writerows(data)

print("Data extraction complete. CSV file created.")